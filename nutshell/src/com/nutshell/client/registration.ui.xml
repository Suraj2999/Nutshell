<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	
	<g:HTMLPanel>
		<m:MaterialPanel textAlign="CENTER" >
	<m:MaterialPanel textAlign="LEFT"  paddingLeft="300" paddingRight="300" ui:field="mp" paddingTop="50">
				<m:MaterialTextBox  ui:field="t1" placeholder="Name" iconType="FACE"></m:MaterialTextBox>
				<m:MaterialTextBox  ui:field="t2" placeholder="Email Id" iconType="ACCOUNT_CIRCLE"></m:MaterialTextBox>
				<m:MaterialTextBox  ui:field="t3" placeholder="News Agency" iconType="DOMAIN"></m:MaterialTextBox>
				<m:MaterialTextBox ui:field="t4" placeholder="Phone" type="NUMBER" iconType="CALL"></m:MaterialTextBox>
				<m:MaterialTextBox  ui:field="t5" placeholder="Password" type="PASSWORD" iconType="LOCK"></m:MaterialTextBox>
				<m:MaterialTextBox  ui:field="t6" placeholder="Confirm Password" type="PASSWORD" iconType="LOCK"></m:MaterialTextBox>
				</m:MaterialPanel>
				<m:MaterialPanel textAlign="CENTER" paddingBottom="10">
			<m:MaterialRow>
			<m:MaterialButton ui:field="b1" text="Submit" waves="LIGHT" backgroundColor="RED" textColor="WHITE" iconType="DEFAULT" iconPosition="RIGHT"/>
			</m:MaterialRow>
			
			</m:MaterialPanel>
		
		</m:MaterialPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
	
	
	
	
	
	
		